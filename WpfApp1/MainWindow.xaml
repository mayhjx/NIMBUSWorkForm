<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="NIMBUS移液平台工作清单生成" Height="550" Width="1200" WindowStartupLocation="Manual">
    <StackPanel Margin="10">
        <StackPanel Margin="0,0,0,10" Orientation="Horizontal">
            <Label>移液操作清单：</Label>
            <TextBox x:Name="FileName" Width="250" Padding="5" IsReadOnly="True"></TextBox>
            <Label Margin="10,0,0,0">每日操作清单：</Label>
            <TextBox x:Name="FileName1" Width="250" Padding="5" IsReadOnly="True"></TextBox>
            <Button x:Name="OpenFileButton"  Width="80" Height="30" Click="OpenFileButton_Click" Content="打开文件" Margin="10,0,10,0" />
            <Button x:Name="PrintButton"  Width="150" Height="30" Click="PrintButton_Click" Content="打印96孔板工作清单" IsEnabled="False" Margin="0,0,10,0"/>
            <Button x:Name="OutputButton"  Width="150" Height="30" Click="OutputButton_Click" Content="导出上机列表和警告信息" IsEnabled="False"/>
        </StackPanel>
        <FlowDocumentPageViewer x:Name="DocumentPage">
        </FlowDocumentPageViewer>
    </StackPanel>
</Window>
